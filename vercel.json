{
  "framework": "astro",
  "installCommand": "yarn install",
  "buildCommand": "yarn run build",
  "outputDirectory": "dist",
  "git": {
    "deploymentEnabled": {
      "main-vercel": true
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}